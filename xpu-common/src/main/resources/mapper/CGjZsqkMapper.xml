        <result property="xyzyId"    column="xyzy_id"    />
        <result property="zsjhNum"    column="zsjh_num"    />
        <result property="sjlqNum"    column="sjlq_num"    />
        <result property="fbqk"    column="fbqk"    />
        <result property="dyzylqNum"    column="dyzylq_num"    />
        <result property="sjbdNum"    column="sjbd_num"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <association property="zysz"    column="zy_id" javaType="CGjZysz" resultMap="CGjZyszResult" />
        <association property="dept"    column="dept_id" javaType="SysDept" resultMap="SysDeptResult" />
<!--        <association property="deptId"    column="dept_id" javaType="Long"/>-->
