            <if test="zsjhNum != null">zsjh_num,</if>
            <if test="sjlqNum != null">sjlq_num,</if>
            <if test="fbqk != null">fbqk,</if>
            <if test="dyzylqNum != null">dyzylq_num,</if>
            <if test="sjbdNum != null">sjbd_num,</if>
