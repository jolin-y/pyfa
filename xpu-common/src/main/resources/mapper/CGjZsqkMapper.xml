            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="xyzyId != null">#{xyzyId},</if>
            <if test="zsjhNum != null">#{zsjhNum},</if>
            <if test="sjlqNum != null">#{sjlqNum},</if>
            <if test="fbqk != null">#{fbqk},</if>
            <if test="dyzylqNum != null">#{dyzylqNum},</if>
            <if test="sjbdNum != null">#{sjbdNum},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateCGjZsqk" parameterType="CGjZsqk">
        update c_gj_zsqk
        <trim prefix="SET" suffixOverrides=",">
            <if test="xyzyId != null">xyzy_id = #{xyzyId},</if>
            <if test="zsjhNum != null">zsjh_num = #{zsjhNum},</if>
            <if test="sjlqNum != null">sjlq_num = #{sjlqNum},</if>
            <if test="fbqk != null">fbqk = #{fbqk},</if>
            <if test="dyzylqNum != null">dyzylq_num = #{dyzylqNum},</if>
            <if test="sjbdNum != null">sjbd_num = #{sjbdNum},</if>
