    <select id="selectCGjZsqkByZsqkId" parameterType="Long" resultMap="CGjZsqkResult">
        select zs.zsqk_id, zs.xyzy_id, zs.zsjh_num, zs.sjlq_num, zs.fbqk, zs.dyzylq_num, zs.sjbd_num, zs.create_by, zs.create_time, zs.update_by, zs.update_time,dept.dept_id from c_gj_zsqk zs
        left join c_gj_zysz zysz on zs.xyzy_id = zysz.zy_id
        LEFT JOIN sys_dept dept ON zysz.dept_id = dept.dept_id
        where zs.zsqk_id = #{zsqkId}
    </select>
        
