    <sql id="selectCGjZsqkVo">
        select zsqk_id, xyzy_id, zsjh_num, sjlq_num, fbqk, dyzylq_num, sjbd_num, create_by, create_time, update_by, update_time from c_gj_zsqk
    </sql>

<!--    <include refid="selectCGjZsqkVo"/>-->
    <select id="selectCGjZsqkList" parameterType="CGjZsqk" resultMap="CGjZsqkResult">
        select zs.zsqk_id, zs.xyzy_id, zs.zsjh_num, zs.sjlq_num, zs.fbqk, zs.dyzylq_num, zs.sjbd_num, zs.create_by, zs.create_time, zs.update_by, zs.update_time, zysz.zy_name, dept.dept_id from c_gj_zsqk zs
        left join c_gj_zysz zysz on zs.xyzy_id = zysz.zy_id
        LEFT JOIN sys_dept dept ON zysz.dept_id = dept.dept_id
        <where>  
            <if test="xyzyId != null "> and xyzy_id = #{xyzyId}</if>
            <if test="zsjhNum != null "> and zsjh_num = #{zsjhNum}</if>
            <if test="sjlqNum != null "> and sjlq_num = #{sjlqNum}</if>
            <if test="fbqk != null "> and fbqk = #{fbqk}</if>
            <if test="dyzylqNum != null "> and dyzylq_num = #{dyzylqNum}</if>
            <if test="sjbdNum != null "> and sjbd_num = #{sjbdNum}</if>
            <if test="dept != null ">
                <if test="dept.deptId != null ">dept.dept_id = #{dept.deptId}</if>
            </if>
        </where>
    </select>
<!--    <if test="zysz.deptId != null "> and zysz.dept_id = #{zysz.deptId}</if>-->
<!--    <if test="deptId != null "> and dept.dept_id = #{deptId}</if>-->



