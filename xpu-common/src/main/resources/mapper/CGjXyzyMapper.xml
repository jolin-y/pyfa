    <sql id="selectCGjXyzyVo">
        select xyzy_id, parent_id, ancestors, xyzy_daima, xyzy_name, xyzy_short_name, xyzy_zydm, order_num, xkml, zylb, remarks, zy_creat_year, create_by, create_time, update_by, update_time from c_gj_xyzy
    </sql>

    <select id="selectCGjXyzyList" parameterType="CGjXyzy" resultMap="CGjXyzyResult">
        <include refid="selectCGjXyzyVo"/>
        <where>  
            <if test="parentId != null "> and parent_id = #{parentId}</if>
            <if test="ancestors != null  and ancestors != ''"> and ancestors = #{ancestors}</if>
            <if test="xyzyDaima != null "> and xyzy_daima = #{xyzyDaima}</if>
            <if test="xyzyName != null  and xyzyName != ''"> and xyzy_name = #{xyzyName}</if>
            <if test="xyzyShortName != null  and xyzyShortName != ''"> and xyzy_short_name = #{xyzyShortName}</if>
