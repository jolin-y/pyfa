            <if test="xkml != null  and xkml != ''"> and xkml = #{xkml}</if>
            <if test="zylb != null  and zylb != ''"> and zylb = #{zylb}</if>
            <if test="remarks != null  and remarks != ''"> and remarks = #{remarks}</if>
            <if test="zyCreatYear != null  and zyCreatYear != ''"> and zy_creat_year = #{zyCreatYear}</if>
        </where>
