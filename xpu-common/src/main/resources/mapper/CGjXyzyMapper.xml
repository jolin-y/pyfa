    <select id="selectCGjXyzyByXyzyId" parameterType="Long" resultMap="CGjXyzyResult">
        <include refid="selectCGjXyzyVo"/>
        where xyzy_id = #{xyzyId}
    </select>


    <sql id="selectCGjXyzyVoByXyzyDaima">
        select xyzy_id, parent_id, ancestors, xyzy_daima, xyzy_name, xyzy_short_name, xyzy_zydm, order_num, xkml, zylb, remarks, zy_creat_year, create_by, create_time, update_by, update_time from c_gj_xyzy where xyzy_daima is not null
    </sql>

        
    <insert id="insertCGjXyzy" parameterType="CGjXyzy" useGeneratedKeys="true" keyProperty="xyzyId">
        insert into c_gj_xyzy
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="parentId != null">parent_id,</if>
            <if test="ancestors != null">ancestors,</if>
            <if test="xyzyDaima != null">xyzy_daima,</if>
            <if test="xyzyName != null">xyzy_name,</if>
            <if test="xyzyShortName != null">xyzy_short_name,</if>
            <if test="xyzyZydm != null">xyzy_zydm,</if>
            <if test="orderNum != null">order_num,</if>
            <if test="xkml != null">xkml,</if>
            <if test="zylb != null">zylb,</if>
            <if test="remarks != null">remarks,</if>
