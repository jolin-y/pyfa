    <delete id="deleteCGjXyzyByXyzyId" parameterType="Long">
        delete from c_gj_xyzy where xyzy_id = #{xyzyId}
    </delete>

    <delete id="deleteCGjXyzyByXyzyIds" parameterType="String">
        delete from c_gj_xyzy where xyzy_id in 
        <foreach item="xyzyId" collection="array" open="(" separator="," close=")">
            #{xyzyId}
        </foreach>
    </delete>



    <select id="selectChildrenXyzyById" parameterType="Long" resultMap="CGjXyzyResult">
		select * from c_gj_xyzy where find_in_set(#{xyzyId}, ancestors)
	</select>

    <update id="updateXyzyChildren" parameterType="java.util.List">
        update c_gj_xyzy set ancestors =
        <foreach collection="xyzys" item="item" index="index"
                 separator=" " open="case xyzy_id" close="end">
            when #{item.xyzyId} then #{item.ancestors}
        </foreach>
        where xyzy_id in
        <foreach collection="xyzys" item="item" index="index"
                 separator="," open="(" close=")">
            #{item.xyzyId}
        </foreach>
    </update>

    <select id="selectCGjXyzyByXyzyDaima" parameterType="CGjXyzy" resultMap="CGjXyzyResult">
        <include refid="selectCGjXyzyVoByXyzyDaima"/>
        <where>
            <if test="xyzyDaima != null and xyzyDaima != ''"> and xyzy_daima = #{xyzyDaima}</if>
        </where>
