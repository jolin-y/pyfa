    <update id="updateCGjXyzy" parameterType="CGjXyzy">
        update c_gj_xyzy
        <trim prefix="SET" suffixOverrides=",">
            <if test="parentId != null">parent_id = #{parentId},</if>
            <if test="ancestors != null">ancestors = #{ancestors},</if>
            <if test="xyzyDaima != null">xyzy_daima = #{xyzyDaima},</if>
            <if test="xyzyName != null">xyzy_name = #{xyzyName},</if>
            <if test="xyzyShortName != null">xyzy_short_name = #{xyzyShortName},</if>
            <if test="xyzyZydm != null">xyzy_zydm = #{xyzyZydm},</if>
